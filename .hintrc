{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/aria": [
      "default",
      {
        "aria-input-field-name": "off",
        "aria-required-attr": "off"
      }
    ],
    "axe/structure": "off",
    "axe/parsing": "off",
    "compat-api/css": "off",
    "css-prefix-order": "off"
  }
}